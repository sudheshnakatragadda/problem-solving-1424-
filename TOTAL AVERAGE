#include <stdio.h>
int main()
{
	int math,chem,hindi,phy,eng,total;
	double avg,per;
	
	printf("enter the value of 5 subjects");
	scanf("%d%d%d%d%d",&math,&chem,&phy,&hindi,&eng);
	
	total=math+chem+hindi+phy+eng;
	printf("total marks :%d",total);
	
	avg=total/5;
	printf("avg marks :%f",avg);
	
	per=total/5*100;
	printf("per :%f",per);
	return 0;

}
